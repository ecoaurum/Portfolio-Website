{
    "version": 2,
    "builds": [
        { "src": "./server.js", "use": "@vercel/node" }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "/server.js",
            "headers": [
                {
                  "key": "access-control-allow-origin",
                  "value": "*"
                }
            ]
        }
    ]
}